    "gulp-uglify": "^1.4.1",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "isparta": "^3.1.0",
    "istanbul": "^0.3.21",
    "jquery": "^2.1.4",
    "jsdom": "^6.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "mocha": "^2.3.3",
    "react-tools": "^0.13.3",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0",
    "yargs": "^3.26.0"
  },
  "scripts": {
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "test": "./node_modules/.bin/mocha ./test/**/*.spec.js --compilers jsx:babel/register,js:babel/register:babel/register --recursive -w",
    "lint": "./node_modules/.bin/eslint ./app --ext .js --ext .jsx",
    "bower": "./node_modules/.bin/bower install"
  }
