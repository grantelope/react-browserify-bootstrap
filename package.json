{
  "name": "jspmreactbootstrap",
  "version": "0.0.1",
  "browser": {
    "modernizr": "./bower_components/modernizr/modernizr.js"
  },
  "browserify-shim": {
    "modernizr": {
      "exports": "modernizr"
    }
  },
  "browserify": {
    "transform": [
      [
        "browserify-shim"
      ],
      [
        "babelify",
        {
          "experimental": true
        }
      ]
    ]
  },
  "description": "jspm react and flux bootstrap",
  "dependencies": {
    "alt": "^0.17.3",
    "axios": "^0.7.0",
    "lodash": "^3.10.1",
    "page": "^1.6.3",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "bower": "^1.5.3",
    "browserify": "^11.2.0",
    "browserify-shim": "^3.8.10",
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "del": "^2.0.2",
    "eslint": "^1.5.1",
    "eslint-plugin-react": "^3.5.1",
    "git-rev-sync": "git+https://github.com/kurttheviking/git-rev-sync.git",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.2",
    "gulp-base64": "^0.1.3",
    "gulp-gzip": "^1.2.0",
    "gulp-if": "^2.0.0",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
